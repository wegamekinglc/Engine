<?xml version="1.0"?>
<ORE>
  <Setup>
    <Parameter name="asofDate">2016-02-05</Parameter>
    <Parameter name="inputPath">Input</Parameter>
    <Parameter name="outputPath">Output</Parameter>
    <Parameter name="logFile">log_20.txt</Parameter>
    <Parameter name="logMask">255</Parameter>
    <Parameter name="marketDataFile">../../Input/market_20160205.txt</Parameter>
    <Parameter name="fixingDataFile">../../Input/fixings_20160205.txt</Parameter>
    <Parameter name="implyTodaysFixings">N</Parameter>
    <Parameter name="curveConfigFile">../../Input/curveconfig.xml</Parameter>
    <Parameter name="conventionsFile">../../../Examples/Input/conventions.xml</Parameter>
    <Parameter name="marketConfigFile">../../Input/todaysmarket.xml</Parameter>
    <Parameter name="pricingEnginesFile">../../Input/pricingengine.xml</Parameter>
    <Parameter name="portfolioFile">portfolio_cpr_20.xml</Parameter>
    <Parameter name="observationModel">None</Parameter>
  </Setup>
  <Markets>
    <Parameter name="lgmcalibration">collateral_inccy</Parameter>
    <Parameter name="fxcalibration">xois_eur</Parameter>
    <Parameter name="pricing">xois_eur</Parameter>
    <Parameter name="simulation">xois_eur</Parameter>
    <Parameter name="sensitivity">xois_eur</Parameter>
  </Markets>
  <Analytics>
    <Analytic type="npv">
      <Parameter name="active">Y</Parameter>
      <Parameter name="baseCurrency">EUR</Parameter>
      <Parameter name="outputFileName">npv_20.csv</Parameter>
    </Analytic>
    <Analytic type="cashflow">
      <Parameter name="active">Y</Parameter>
      <Parameter name="outputFileName">flows_20.csv</Parameter>
    </Analytic>
    <Analytic type="curves">
      <Parameter name="active">Y</Parameter>
      <Parameter name="configuration">default</Parameter>
      <Parameter name="grid">240,1M</Parameter>
      <Parameter name="outputFileName">curves_20.csv</Parameter>
    </Analytic>
    <Analytic type="sensitivity">
      <Parameter name="active">Y</Parameter>
      <Parameter name="marketConfigFile">simulation.xml</Parameter>
      <Parameter name="sensitivityConfigFile">sensitivity.xml</Parameter>
      <Parameter name="pricingEnginesFile">../../Input/pricingengine.xml</Parameter>
      <Parameter name="scenarioOutputFile">sensi_scenarios_20.csv</Parameter>
      <Parameter name="sensitivityOutputFile">zero_sensi_20.csv</Parameter>
      <Parameter name="crossGammaOutputFile">crossgamma_20.csv</Parameter>
      <Parameter name="recalibrateModels">Y</Parameter>
      <Parameter name="outputSensitivityThreshold">0.000001</Parameter>
      <Parameter name="parSensitivity">Y</Parameter>
      <Parameter name="parSensitivityOutputFile">par_sensi_20.csv</Parameter>
      <Parameter name="outputJacobi">N</Parameter>
      <Parameter name="jacobiOutputFile">jacobi.csv</Parameter>
      <Parameter name="jacobiInverseOutputFile">jacobi_inverse.csv</Parameter>
    </Analytic>
  </Analytics>
</ORE>
