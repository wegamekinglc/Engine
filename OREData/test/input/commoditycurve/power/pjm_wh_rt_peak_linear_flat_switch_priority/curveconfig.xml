  <CommodityCurves>
    <CommodityCurve>
    <CurveId>ICE:PDQ</CurveId>
    <CurveDescription>PJM Western Hub Real Time Peak, good NERC weekdays</CurveDescription>
    <Currency>USD</Currency>
    <PriceSegments>
      <PriceSegment>
        <Type>Future</Type>
        <Priority>2</Priority>
        <Conventions>ICE:PDQ</Conventions>
        <Quotes>
          <Quote>COMMODITY_FWD/PRICE/ICE:PDQ/*</Quote>
        </Quotes>
      </PriceSegment>
      <PriceSegment>
        <Type>AveragingFuture</Type>
        <Priority>1</Priority>
        <Conventions>ICE:PMI</Conventions>
        <Quotes>
          <Quote>COMMODITY_FWD/PRICE/ICE:PMI/*</Quote>
        </Quotes>
      </PriceSegment>
    </PriceSegments>
    <DayCounter>A365</DayCounter>
    <InterpolationMethod>LinearFlat</InterpolationMethod>
    <Extrapolation>true</Extrapolation>
    <BootstrapConfig>
      <DontThrow>true</DontThrow>
      <MaxAttempts>5</MaxAttempts>
      <MaxFactor>1.5</MaxFactor>
      <MinFactor>1.5</MinFactor>
      <DontThrowSteps>5</DontThrowSteps>
    </BootstrapConfig>
  </CommodityCurve>
  </CommodityCurves>
  <CommodityVolatilities/>
  <Correlations/>
